            position: absolute;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        .btn_contianer button{
            width: 150px;
            height: 35px;
            border-radius: 4px;
            border: none;
            background-color: hotpink;
            color: #fff;
            font-size: 18px;
        }
        .btn_contianer button:active{
            background-color: honeydew;
            color: black;
        }
        .btn_contianer button:focus{
            outline: none;
            border: none;

        }
